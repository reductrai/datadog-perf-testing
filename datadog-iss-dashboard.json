                  "right_yaxis": true
                }
              ],
              "yaxis": {
                "label": "¬∞C",
                "scale": "linear",
                "min": "18",
                "max": "26"
              },
              "right_yaxis": {
                "label": "%",
                "scale": "linear",
                "min": "30",
                "max": "70"
              }
            }
          }
        ]
      },
      "layout": {
        "x": 0,
        "y": 3,
        "width": 12,
        "height": 4
      }
    },
    {
