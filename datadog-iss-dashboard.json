    {
      "id": 9,
      "definition": {
        "title": "üõ∞Ô∏è Navigation & Orbit",
        "type": "group",
        "background_color": "vivid_purple",
        "layout_type": "ordered",
        "widgets": [
          {
            "id": 901,
            "definition": {
              "title": "Orbital Altitude",
