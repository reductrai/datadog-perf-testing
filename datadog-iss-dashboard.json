              ],
              "yaxis": {
                "label": "km",
                "scale": "linear",
                "min": "405",
                "max": "411"
              },
              "markers": [
                {
                  "value": "y = 408",
                  "display_type": "ok dashed",
                  "label": "Target"
                }
              ]
            }
          },
          {
            "id": 902,
            "definition": {
              "title": "Attitude Control",
              "type": "timeseries",
              "requests": [
                {
                  "q": "avg:nasa.iss.navigation.attitude_pitch{*}",
                  "display_type": "line",
                  "style": {
                    "palette": "blue"
                  },
                  "metadata": [
                    {
                      "alias": "Pitch"
                    }
                  ]
                },
                {
                  "q": "avg:nasa.iss.navigation.attitude_roll{*}",
                  "display_type": "line",
                  "style": {
                    "palette": "green"
                  },
                  "metadata": [
                    {
                      "alias": "Roll"
