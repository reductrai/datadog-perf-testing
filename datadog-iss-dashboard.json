                    },
                    {
                      "comparator": ">",
                      "value": 1,
                      "palette": "white_on_red"
                    }
                  ]
                }
              ],
              "custom_unit": "%",
              "precision": 2
            }
          },
          {
            "id": 1004,
            "definition": {
              "title": "Ground Station Links",
              "type": "hostmap",
              "requests": {
                "fill": {
                  "q": "avg:nasa.iss.communication.ground_station_active{*} by {host}"
                }
              },
              "node_type": "host",
              "no_metric_hosts": false,
              "no_group_hosts": true,
              "style": {
                "palette": "green_to_orange",
                "palette_flip": false
              }
            }
          }
        ]
      },
      "layout": {
        "x": 0,
        "y": 19,
        "width": 12,
        "height": 4
