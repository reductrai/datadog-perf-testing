        "widgets": [
          {
            "id": 1001,
            "definition": {
              "title": "Ku-Band Signal Strength",
              "type": "timeseries",
              "requests": [
                {
                  "q": "avg:nasa.iss.communication.ku_band_signal_strength{*}",
                  "display_type": "line",
                  "style": {
